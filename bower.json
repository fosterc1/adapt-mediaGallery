{
  "name": "adapt-mediaGallery",
  "version": "1.0.0",
  "framework": ">=5.8",
  "homepage": "https://github.com/cgkineo/adapt-mediaGallery",
  "issues": "https://github.com/cgkineo/adapt-mediaGallery/issues/ ",
  "component": "mediagallery",
  "displayName": "Media Gallery",
  "description": "Extends the functionality of adapt-contrib-media by providing a gallery of videos",
  "main": "/js/adapt-mediaGallery.js",
  "keywords": [
    "adapt-plugin",
    "adapt-component"
  ],
  "pluginDependencies": {
    "adapt-contrib-media": ">=5.1"
  },
  "license": "GPL-3.0",
  "targetAttribute": "_items",
  "assetFields": [
    "_items[]._graphic.src",
    "_items[]._media.mp4",
    "_items[]._media.ogv",
    "_items[]._media.webm",
    "_items[]._media.mp3",
    "_items[]._media.poster",
    "_items[]._media.cc[].src",
    "_transcript.transcriptLink"
  ]
}
